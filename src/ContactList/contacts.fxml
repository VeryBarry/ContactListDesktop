<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="453.0" prefWidth="429.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ContactList.Controller">
   <children>
      <HBox alignment="CENTER" prefHeight="20.0" prefWidth="429.0">
         <children>
            <TextField fx:id="nameTextF" alignment="CENTER" promptText="First/Last Name" />
            <TextField fx:id="phoneTextF" alignment="CENTER" promptText="Phone Number" />
            <TextField fx:id="emailTextF" alignment="CENTER" promptText="Email Address" />
            <Button alignment="CENTER" minWidth="-Infinity" mnemonicParsing="false" onAction="#addContact" text="Add" />
            <Button alignment="CENTER" minWidth="-Infinity" mnemonicParsing="false" onAction="#removeContact" text="Remove" />
         </children>
      </HBox>
      <ListView fx:id="contactListView" prefHeight="437.0" prefWidth="429.0" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
